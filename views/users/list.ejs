<% include ../partials/header%>

<div class="container-fluid">
    <div class="card border-light mx-1">
        <div class="card-body">
            <div class="card border-light">
                <h4 class="card-title bg-transparent display-4">Filters</h4>
                <div class="card-body">
                    <form class="form-horizontal" action="" method="GET">
                        <!-- ==================== FILTER ID ======================== -->

                        <div class="form-group row">
                            <div class="custom-control custom-checkbox col-12 col-sm-2">
                                <input type="checkbox" class="custom-control-input" for="" id="customCheck1">
                                <label class="custom-control-label" for="customCheck1" id=""
                                    name="checkprojectid">ID</label>
                            </div>
                            <div class="col-12 col-sm-9">
                                <input type="text" class="form-control" id="userId" placeholder="ID" name="userId">
                            </div>
                        </div>

                        <!-- ==================== FILTER EMAIL ======================== -->

                        <div class="form-group row">
                            <div class="custom-control custom-checkbox col-12 col-sm-2">
                                <input type="checkbox" class="custom-control-input" for="" id="customCheck2">
                                <label class="custom-control-label" for="customCheck2" id=""
                                    name="checkpjname">Email</label>
                            </div>
                            <div class="col-12 col-sm-9">
                                <input type="email" class="form-control" id="userEmail" placeholder="Email"
                                    name="userEmail">
                            </div>
                        </div>

                        <!-- ==================== FILTER NAME ======================== -->

                        <div class="form-group row">
                            <div class="custom-control custom-checkbox col-12 col-sm-2">
                                <input type="checkbox" class="custom-control-input" for="" id="customCheck3">
                                <label class="custom-control-label" for="customCheck3" id=""
                                    name="checkpjname">Name</label>
                            </div>
                            <div class="col-12 col-sm-9">
                                <input type="text" class="form-control" id="userName" placeholder="Name"
                                    name="userName">
                            </div>
                        </div>

                        <!-- ==================== FILTER ROLE ======================== -->

                        <div class="form-group row">
                            <div class="custom-control custom-checkbox col-12 col-sm-2">
                                <input type="checkbox" class="custom-control-input" for="" id="customCheck4">
                                <label class="custom-control-label" for="customCheck4" id=""
                                    name="userRole">Role</label>
                            </div>
                            <div class="col-12 col-sm-9">
                                <select class="form-control" id="userRole" name="userRole">
                                    <option disabled selected>Choose the Role ...</option>
                                    <option value="">duar</option>
                                    <option value="">nmax</option>
                                    <!--  data.forEach((item) => { 
                                <option >
                                     item.firstmembername 
                                </option>
                             })  -->
                                </select>
                            </div>
                        </div>

                        <!-- ==================== FILTER TYPE JOB ======================== -->

                        <div class="form-group row">
                            <div class="custom-control custom-checkbox col-12 col-sm-2">
                                <input type="checkbox" class="custom-control-input" for="" id="customCheck4">
                                <label class="custom-control-label" for="customCheck4" id="" name="userTypejob">Type
                                    Job</label>
                            </div>
                            <div class="col-12 col-sm-9">
                                <select class="form-control" id="userTypejob" name="userTypejob">
                                    <option disabled selected>Choose the Type Job ...</option>
                                    <option value="">duar</option>
                                    <option value="">nmax</option>
                                    <!--  data.forEach((item) => { 
                                <option >
                                     item.firstmembername 
                                </option>
                             })  -->
                                </select>
                            </div>
                        </div>

                        <!-- ==================== BUTTON ======================== -->

                        <div class="form-group row">
                            <div class="col-sm-10">
                                <button type="button" class="btn btn-primary" role="button">Search</button>
                                <button type="button" class="btn btn-white"> <a href="/projects">Reset</a></button>
                            </div>
                        </div>
                    </form>

                    <!-- ==================== OPTION ======================== -->

                    <div class="card border-light mx-1">
                        <div class="card-body">
                            <div class="card border-light">
                                <h4 class="card-title bg-transparent display-4">Options</h4>
                                <div class="card-body">
                                    <form action="" method="POST" class="form-check">
                                        <div class="form-group row">
                                            <h5 class="col-12 col-sm-5">
                                                Columns
                                            </h5>
                                            <div class="row col-12 col-sm-7">
                                                <div class="custom-control custom-checkbox col-12 col-sm-7">
                                                    <input type="checkbox" class="custom-control-input" for=""
                                                        id="opcheck1">
                                                    <label class="custom-control-label" for="opcheck1" id="opid"
                                                        name="opid">ID</label>
                                                </div>
                                                <div class="custom-control custom-checkbox col-12 col-sm-7">
                                                    <input type="checkbox" class="custom-control-input" for=""
                                                        id="opcheck2">
                                                    <label class="custom-control-label" for="opcheck2" id="opname"
                                                        name="opname">Name</label>
                                                </div>
                                                <div class="custom-control custom-checkbox col-12 col-sm-7">
                                                    <input type="checkbox" class="custom-control-input" for=""
                                                        id="opcheck3">
                                                    <label class="custom-control-label" for="opcheck3" id="opmembers"
                                                        name="opmembers">Members</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-10">
                                            <button type="submit" class="btn btn-outline-info">Apply</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- ==================== TABLES ======================== -->
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">Email</th>
                                <th scope="col">Name</th>
                                <th scope="col">TypeJob</th>
                                <th scope="col">Roles</th>
                                <th scope="col">Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td>
                                    <a href="/projects/edit/" class="btn btn-success">EDIT</a>
                                    <a href="/projects/delete/" class="btn btn-danger">DELETE</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="form-group row">
                    <div class="col-sm-10">
                        <a href="/projects/add" type="button" class="btn btn-primary">ADD</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
<% include ../partials/footer%>